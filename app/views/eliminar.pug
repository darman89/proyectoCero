#deleteModal(class="modal" tabindex="-1" role="dialog")
    div(class="modal-dialog" role="document")
      div(class="modal-content")
        div(class="modal-header")
          h5(class="modal-title") Eliminar Evento
          button(type="button" class="close" data-dismiss="modal" aria-hidden="Close")
            span(aria-hidden="true") &times;
        div(class="modal-body")
          p Seguro desea eliminar el evento: 
            span(id="nombre_evento")
        div(class="modal-footer")
          button(id="eliminar_evento" type="button" class="btn btn-danger") Eliminar
          button(id="cancelar" type="button" class="btn btn-default" data-dismiss="modal") Cancelar